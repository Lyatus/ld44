{
  "name": "ld44",
  "version": 2,
  "code": [
    "const ground_y = 96;",
    "const max_health = 50;",
    "const min_fighter_dist = 6;",
    "",
    "const p1_start_x = 48;",
    "const p2_start_x = 96;",
    "",
    "function init() {",
    "  startmenu_init();",
    "}",
    "",
    "// Start menu functions /////////////////////////////////////////",
    "",
    "function startmenu_init() {",
    "  startmenu_choice = 0;",
    "  update = startmenu_update;",
    "}",
    "function startmenu_update() {",
    "  const choices = [",
    "    fight_init,",
    "  ];",
    "  ",
    "  if(btn(4)) {",
    "    fight_init();",
    "  }",
    "  startmenu_draw();",
    "}",
    "function startmenu_draw() {",
    "  cls(0);",
    "  circfill(64, 64, 64, 1);",
    "}",
    "",
    "// Fight functions /////////////////////////////////////////////",
    "",
    "class fighter {",
    "  constructor(index) {",
    "    this.index = index;",
    "    this.flip = index > 0;",
    "    this.health = max_health;",
    "    this.x = index == 0 ? p1_start_x : p2_start_x;",
    "    this.y = ground_y;",
    "    this.spr = 0;",
    "    this.blocking = false;",
    "    this.moving = false;",
    "    this.crouched = false;",
    "    this.frame = 0;",
    "  }",
    "  update() {",
    "    this.input();",
    "    if(this.blocking) {",
    "      this.spr = this.crouched ? 7 : 6;",
    "    } else if(this.moving) { // Walk cycle",
    "      this.spr = 2 + (this.frame % 16) / 4;",
    "    } else { // Idle",
    "      if(this.crouched) { // Crouched idle",
    "        this.spr = 0;",
    "      } else { // Standing idle",
    "        this.spr = (this.frame % 16) < 8;",
    "      }",
    "    }",
    "    this.moving = false;",
    "    this.frame++;",
    "  }",
    "  draw() {",
    "    palm(6, this.index == 0 ? 6 : 7); // Set player color",
    "    spr(this.spr, this.x - 4, this.y - 16, 1, 2, this.flip);",
    "    palm(6, 6); // Reset palette",
    "  }",
    "  other_fighter() {",
    "    return fighters[1 - this.index];",
    "  }",
    "  move(dx) {",
    "    // Respect minimum distance between fighters",
    "    // This avoids fighters crossing each other",
    "    const dist = this.dist_to_other();",
    "    if(dist <= min_fighter_dist) {",
    "      if(this.is_right_facing()) {",
    "        dx = min(dx, 0);",
    "      } else {",
    "        dx = max(dx, 0);",
    "      }",
    "    }",
    "    if(dx != 0) {",
    "      this.x += dx;",
    "      this.moving = true;",
    "    }",
    "  }",
    "  punch() {",
    "    ",
    "  }",
    "  dist_to_other() {",
    "    return abs(this.x - this.other_fighter().x);",
    "  }",
    "  is_right_facing() {",
    "    return this.index == 0;",
    "  }",
    "}",
    "",
    "class player_fighter extends fighter {",
    "  input() {",
    "    let other = this.other_fighter();",
    "    let dx = 0;",
    "    if(btn(0, this.index)) dx -= 1;",
    "    if(btn(1, this.index)) dx += 1;",
    "    ",
    "    this.blocking = btn(5, this.index);",
    "    this.crouched = btn(3, this.index);",
    "    ",
    "    this.move(dx);",
    "  }",
    "}",
    "",
    "class computer_fighter extends fighter {",
    "  input() {",
    "    let other = this.other_fighter();",
    "    this.move(rnd(3) - 1);",
    "  }",
    "}",
    "",
    "function fight_init() {",
    "  fighters = [",
    "    new player_fighter(0),",
    "    new computer_fighter(1),",
    "  ];",
    "",
    "  update = fight_update;",
    "}",
    "function fight_update() {",
    "  let fighter_ave_x = 0;",
    "  // Update fighters",
    "  fighters.forEach(function(fighter) {",
    "    fighter.update();",
    "    fighter_ave_x += fighter.x;",
    "  });",
    "  fighter_ave_x /= 2;",
    "  ",
    "  cam_x = mid(0, 32, fighter_ave_x - 64);",
    "  ",
    "  fight_draw();",
    "}",
    "function fight_draw() {",
    "  cls(0);",
    "  ",
    "  // Camera",
    "  cam(cam_x, 0);",
    "  ",
    "  // Draw background",
    "  map(0, 0, 0, 0, 20, 16);",
    "  ",
    "  // Draw fighters",
    "  fighters.forEach(function(fighter) {",
    "    fighter.draw();",
    "  });",
    "  ",
    "  cam(0, 0); // Reset cam for UI",
    "",
    "  // Draw health bar backgrounds",
    "  rectfill(1, 1, max_health, 2, 0);",
    "  rectfill(127 - max_health, 1, max_health, 2, 0);",
    "  ",
    "  // Draw health bars",
    "  rectfill(1, 1, fighters[0].health, 2, 1);",
    "  rectfill(127 - fighters[0].health, 1, fighters[0].health, 2, 1);",
    "}"
  ],
