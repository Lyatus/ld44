  "name": "ld44",
  "version": 2,
  "code": [
    "const ground_y = 96;",
    "const max_health = 50;",
    "",
    "const p1_health_color = 6;",
    "const p2_health_color = 7;",
    "",
    "function init() {",
    "  startmenu_init();",
    "}",
    "",
    "// Start menu functions /////////////////////////////////////////",
    "",
    "function startmenu_init() {",
    "  startmenu_choice = 0;",
    "  update = startmenu_update;",
    "}",
    "function startmenu_update() {",
    "  const choices = [",
    "    fight_init,",
    "  ];",
    "  ",
    "  if(btn(4)) {",
    "    fight_init();",
    "  }",
    "  startmenu_draw();",
    "}",
    "function startmenu_draw() {",
    "  cls(0);",
    "  circfill(64, 64, 64, 1);",
    "}",
    "",
    "// Fight functions /////////////////////////////////////////////",
    "",
    "class fighter {",
    "  constructor(index) {",
    "    this.index = index;",
    "    this.flip = index > 0;",
    "    this.health = max_health;",
    "    this.x = index > 0 ? 96 : 32;",
    "    this.y = ground_y;",
    "    this.spr = 0;",
    "    this.shield = 0; // 0 = none, 1 = bottom, 2 = top",
    "    this.moving = false;",
    "    this.frame = 0;",
    "  }",
    "  update() {",
    "    ",
    "    if(this.moving) {",
    "      // Walk cycle",
    "      this.spr = 2 + (this.frame % 16) / 4;",
    "    } else {",
    "      // Idle",
    "      this.spr = (this.frame % 16) < 8;",
    "    }",
    "    this.moving = false;",
    "    this.frame++;",
    "  }",
    "  draw() {",
    "    spr(this.spr, this.x - 4, this.y - 16, 1, 2, this.flip);",
    "  }",
    "  other_fighter() {",
    "    return fighters[1 - this.index];",
    "  }",
    "  move(dx) {",
    "    this.x += dx;",
    "    if(dx != 0) {",
    "      this.moving = true;",
    "    }",
    "  }",
    "  distance_to_other() {",
    "    return abs(this.x - this.other_fighter().x);",
    "  }",
    "}",
    "",
    "class player_fighter extends fighter {",
    "  input() {",
    "    let other = this.other_fighter();",
    "    let dx = 0;",
    "    if(btn(0, this.index)) dx -= 1;",
    "    if(btn(1, this.index)) dx += 1;",
    "    this.move(dx);",
    "  }",
    "}",
    "",
    "class computer_fighter extends fighter {",
    "  input() {",
    "    super.update();",
    "    let other = this.other_fighter();",
    "    this.move(rnd(3) - 1);",
    "  }",
    "}",
    "",
    "function fight_init() {",
    "  fighters = [",
    "    new player_fighter(0),",
    "    new computer_fighter(1),",
    "  ];",
    "",
    "  update = fight_update;",
    "}",
    "function fight_update() {",
    "  // Update fighters",
    "  fighters.forEach(function(fighter) {",
    "    fighter.input();",
    "    fighter.update();",
    "  });",
    "  ",
    "  fight_draw();",
    "}",
    "function fight_draw() {",
    "  cls(0);",
    "  ",
    "  // Draw background",
    "  ",
    "  // Draw fighters",
    "  fighters.forEach(function(fighter) {",
    "    fighter.draw();",
    "  });",
    "",
    "  // Draw health bar backgrounds",
    "  rectfill(1, 1, max_health, 2, 0);",
    "  rectfill(127 - max_health, 1, max_health, 2, 0);",
    "  ",
    "  // Draw health bars",
    "  rectfill(1, 1, fighters[0].health, 2, 1);",
    "  rectfill(127 - fighters[0].health, 1, fighters[0].health, 2, 1);",
    "}"
  ],
